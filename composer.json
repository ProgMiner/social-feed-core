{
    "name": "progminer/social-feed-core",
    "description": "Library for getting posts from social networks",
    "type": "library",
    "keywords": ["social", "feed", "vk"],
    "license": "MIT",
    "authors": [
        {
            "name": "ProgMiner",
            "email": "eridan200@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "progminer/php-datagen-lib": "^0.1.0@alpha",
        "symfony/config": "^4.0"
    },
    "require-dev": {
        "pimple/pimple": "~3.0",
        "progminer/php-datagen": "^0.2.0@alpha"
    },
    "autoload": {
        "psr-4": {
            "SocialFeedCore\\": "lib"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
